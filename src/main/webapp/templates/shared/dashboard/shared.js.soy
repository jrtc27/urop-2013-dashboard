{namespace shared.dashboard}

/**
 * @param notifications
 */
{template .getNotifications}
	{foreach $notification in $notifications}
	<li class="notification-feed-item">
		<div><form id="{$notification.id}">Message: {$notification.message} | Section: {$notification.section} | Link: {$notification.link} | Read: {$notification.read} | Time: {$notification.timestamp} </form></div>
	</li>
	{/foreach}
{/template}